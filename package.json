{
  "name": "yas-http",
  "version": "1.0.5",
  "description": "Support to send http(s) requests, API document and Http(s) mock server",
  "keywords": [
    "http",
    "https",
    "api",
    "yaml-scene",
    "extensions",
    "dev"
  ],
  "scripts": {
    "install:dist": "yarn install --production --modules-folder ./dist/node_modules",
    "dev": "npx nodemon --watch src --watch test --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --inspect-brk src/index.ts'",
    "test": "npx jest --detectOpenHandles",
    "test:debug": "npx --node-options=\"--inspect-brk\" jest --detectOpenHandles",
    "doc": "npx yaml-scene scenes/guide.yas.yaml",
    "prebuild": "yarn clean",
    "build": "tsc --project tsconfig.build.json",
    "postbuild": "node bin/tsconfig && node bin/update_package",
    "clean": "rm -rf dist",
    "prenewtag": "yarn test && yarn build",
    "newtag": "npx yaml-scene ./scenes/version.yas.yaml",
    "postnewtag": "yarn clean"
  },
  "preferGlobal": true,
  "dependencies": {
    "@koa/cors": "^3.2.0",
    "@koa/router": "^10.1.1",
    "axios": "^0.26.1",
    "curl-generator": "^0.2.0",
    "form-data": "^4.0.0",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-static": "^5.0.0",
    "lodash.merge": "^4.6.2"
  },
  "peerDependencies": {
    "yaml-scene": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/koa": "^2.13.4",
    "@types/node": "^14.14.6",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5",
    "yaml-scene": "^1.1.0"
  },
  "author": {
    "name": "Đoàn Thuận Thành",
    "email": "doanthuanthanh88@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/doanthuanthanh88/yas-http"
  },
  "bugs": {
    "url": "https://github.com/doanthuanthanh88/yas-http/issues",
    "email": "doanthuanthanh88@gmail.com"
  }
}
